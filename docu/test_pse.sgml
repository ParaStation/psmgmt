<refentry id="test_pse">
  <refmeta>
    <refentrytitle>test_pse</refentrytitle>
    <manvolnum>1</manvolnum>
  </refmeta>

  <refnamediv>
    <refname>test_pse</refname>
    <refpurpose>
      test virtual connections within a cluster.
    </refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <cmdsynopsis>
      <command>test_pse</command>
      <arg>-np <replaceable>num</replaceable></arg>
    </cmdsynopsis>
  </refsynopsisdiv>

  <refsect1>
    <title>Description</title>
    <para>
      This command spawns <replaceable>num</replaceable> processes
      within the cluster.
    </para>
    <para>
      It's intended to test the process spawning capabilities of &ps;.
      It does not test any communication facilities within &ps;.
    </para>
  </refsect1>

  <refsect1>
    <title>Options</title>
    <variablelist>
      <varlistentry>
	<term>
	  <option>-np <replaceable>num</replaceable></option>
        </term>
	<listitem>
	  <para>Spawn <replaceable>num</replaceable> processes.
	  </para>
	</listitem>
      </varlistentry>
    </variablelist>
  </refsect1>

  <refsect1>
    <title>See also</title>
    <para>
      <xref linkend="psid"/>
    </para>
  </refsect1>
</refentry>

