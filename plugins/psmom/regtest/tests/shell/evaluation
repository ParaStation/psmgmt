import sys
import os

sys.path.append(os.getenv("RTH"))
from rth import *

r = rth()
rth.parseOutput(r, os.getenv("OUTPUT_FILE"))
rth.testEnv(r, "SHELL", os.getenv("SHELL_LIST"))
