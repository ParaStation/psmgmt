[Unit]
Description=The ParaStation management Daemon
Wants=local-fs.target network-online.target
After=local-fs.target network-online.target

[Service]
Type=forking
IgnoreSIGPIPE=no
ExecStart=@bindir@/psid
Delegate=yes

[Install]
WantedBy=multi-user.target
