#!/bin/sh

echo "Filedescriptions generated by $0"
date
echo 
find . -type f -exec grep -q "\*fdb\*" {} \; -print -exec \
	gawk '/\*fde\*/{desc=0;li=1} {if (desc==1){\
	print}} /\*fdb\*/{desc=1} END{if(li==1){\
	print "***************************************************"}}' {} \;
echo "                                             (2000)JH"
